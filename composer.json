{
  "name": "lamp-io/lio",
  "type": "library",
  "description": "Command line interface, for lamp.io platform",
  "license": "MIT",
  "authors": [],
  "bin": [
    "bin/lio"
  ],
  "require": {
    "symfony/console": "^4.3",
    "art4/json-api-client": "^0.10.0",
    "guzzlehttp/guzzle": "^6.3",
    "php": ">=7.2",
    "ext-json": "*",
    "ext-zip": "*",
    "symfony/finder": "^4.3",
    "padraic/phar-updater": "^1.0.6",
    "symfony/yaml": "^4.3",
    "vlucas/phpdotenv": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3"
  },
  "version": "0.1.0",
  "autoload": {
    "psr-4": {
      "Lio\\": "src"
    },
    "classmap": [
      "src"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "scripts": {
    "build": "php bin/build"
  }
}
